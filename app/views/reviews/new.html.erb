<h1>Review for <%= @review.restaurant.name %></h1>
<div class="container">
  <%= simple_form_for(@review, url: restaurant_reviews_path) do |f| %>
    <%= f.input :rating %>
    <%= f.input :content %>
    <%= f.hidden_field :restaurant_id, value: @review.restaurant_id %>
    <%= f.submit %>
  <% end %>

</div>
